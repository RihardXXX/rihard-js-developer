{
  "name": "rihard-developer.ru",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "predeploy": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy:app": "npm-run-all predeploy deploy",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "analize": "npx nuxt build --analyze"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/component-cache": "^1.1.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.1",
    "core-js": "^3.25.3",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.8",
    "nuxt-lazy-load": "^1.3.0",
    "swiper": "^8.4.2",
    "uniqid": "^5.4.0",
    "vue": "^2.7.10",
    "vue-mq": "^1.0.1",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxt/image": "^0.7.2",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "gh-pages": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "push-dir": "^0.4.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.60.0",
    "sass-loader": "^10.4.1",
    "stylelint": "^14.13.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-webpack-plugin": "^2.1.1"
  }
}
